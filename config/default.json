{
  "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ]
  },
  "firecrawl-mcp": {
    "command": "npx",
    "args": ["-y", "firecrawl-mcp"],
    "env": {
      "FIRECRAWL_API_KEY": ""
    }
  },
  "yfmcp": {
    "command": "uvx",
    "args": ["yfmcp@latest"]
  }
}
