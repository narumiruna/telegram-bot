{
  "playwright": {
    "command": "bunx",
    "args": ["@playwright/mcp@latest"]
  },
  "firecrawl-mcp": {
    "command": "bunx",
    "args": ["-y", "firecrawl-mcp"],
    "env": {
      "FIRECRAWL_API_KEY": ""
    }
  },
  "yfmcp": {
    "command": "uvx",
    "args": ["yfmcp@latest"]
  },
  "gurume": {
    "command": "uvx",
    "args": ["gurume@latest", "mcp"]
  }
}
